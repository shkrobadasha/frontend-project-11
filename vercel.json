{
    "version": 2,
    "builds": [
      {
        "src": "build/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "build/main.*.js",
        "use": "@vercel/static"
      },
      {
        "src": "build/main.*.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }